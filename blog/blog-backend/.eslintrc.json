{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6" : true,
        "node" : true
    },
    "extends": ["eslint:recommended","prettier"],
    "globals" : {
        "Atomics" : "readonly",
        "ShareArrayBuffer" : "readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars" : "warn",
        "no-console" : "off"
    }
}
